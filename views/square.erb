<div id="gameHeadline" class="headline">
  <span>Home: <%= home_team %></span><br/>
  <span>Away: <%= away_team %></span><br/>
  <span>Time: <%= time %></span>
</div>

<div id="square" class="square">
  <%= erb :_square, :layout => false, :locals => {
    :square  => square_values,
    :game_id => game_id
  } %>
</div>
